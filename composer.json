{
    "name": "djvue/d-media-bundle",
    "description": "Symfony bundle for d-media package",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "djvue",
            "email": "vahlovd@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-mbstring": "*",
        "symfony/config": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/filesystem": "^5.2",
        "symfony/routing": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/orm-pack": "^2.1",
        "symfony/framework-bundle": "^5.2",
        "symfony/serializer": "^5.2",
        "symfony/yaml": "^5.2",
        "league/flysystem-bundle": "^2.0",
        "symfony/translation": "^5.2",
        "symfony/mime": "^5.2",
        "symfony/security-bundle": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Djvue\\DMediaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Djvue\\DMediaBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fakerphp/faker": "^1.13",
        "symfony/maker-bundle": "^1.29",
        "jetbrains/phpstorm-attributes": "^1.0",
        "liip/test-fixtures-bundle": "^1.0.0",
        "vimeo/psalm": "^4.4",
        "psalm/plugin-symfony": "^2.1",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/browser-kit": "^5.2",
        "symfony/css-selector": "^5.2",
        "ext-gd": "*"
    }
}
